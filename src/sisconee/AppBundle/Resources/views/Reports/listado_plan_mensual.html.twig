                    <td>{{ e['planFebrero'] }}</td>
                    <td >{{ e['planMarzo'] }}</td>
                    <td >{{ e['planAbril'] }}</td>
                    <td >{{ e['planMayo'] }}</td>
                    <td >{{ e['planJunio'] }}</td>
                    <td >{{ e['planJulio'] }}</td>
                    <td>{{ e['planAgosto'] }}</td>
                    <td >{{ e['planSeptiembre'] }}</td>
                    <td >{{ e['planOctubre'] }}</td>
                    <td >{{ e['planNoviembre'] }}</td>
                    <td >{{ e['planDiciembre'] }}</td>
                    <td > {{  e['planEnero']+ e['planFebrero']+ e['planMarzo']+ e['planAbril']+ e['planMayo']+ e['planJunio']+e['planJulio']+ e['planAgosto']+ e['planSeptiembre']+ e['planOctubre']+ e['planNoviembre']+ e['planDiciembre'] }}</td>
                    {% set TotalPlanEnero = TotalPlanEnero +  e['planEnero'] %}
                    {% set TotalPlanFebrero = TotalPlanFebrero +  e['planFebrero'] %}
                    {% set TotalPlanMarzo = TotalPlanMarzo +  e['planMarzo'] %}
                    {% set TotalPlanAbril = TotalPlanAbril +  e['planAbril'] %}
                    {% set TotalPlanMayo = TotalPlanMayo +  e['planMayo'] %}
